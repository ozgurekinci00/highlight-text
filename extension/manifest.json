{
    "manifest_version": 2,
    "name": "Keyword Underliner",
    "version": "1.0",
    "description": "A Chrome extension to underline specific keywords on a page.",
    "permissions": ["activeTab", "https://*/*", "http://*/*"],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["libs/axios.min.js", "libs/Readability.js", "content.js"],
        "css": ["style.css"]
      }
    ]
  }
  